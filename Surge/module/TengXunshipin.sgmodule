#!name=腾讯视频
#!desc=去除,视频广告


[Rule]
AND,((PROTOCOL,HTTPS), (DOMAIN,iacc.qq.com)),REJECT-NO-DROP



[Script]
腾讯 = type=http-request,pattern=^(https:\/\/i\.video|http:\/\/iacc)\.qq\.com\/$,requires-body=0,max-size=0,script-path=https://raw.githubusercontent.com/chizitiedan/ios_rule_script/main/Surge/script/TengXun.js


[MITM]
hostname = %APPEND% i.video.qq.com,iacc.qq.com
